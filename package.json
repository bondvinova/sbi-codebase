    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.0",
    "react-router-dom": "^6.15.0",
    "react-test-renderer": "^18.2.0",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.22.2",
    "zustand": "^4.4.1"
